{
    "version": 2,
    "name": "server",
    "builds": [
      { "src": "./server.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/data/getPrice",
        "methods": ["POST"],
        "headers": {
          "Content-Type": "application/json",
          "Access-Control-Allow-Origin": "https://casejolt.vercel.app",
          "Access-Control-Allow-Credentials": "true"
        },
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }