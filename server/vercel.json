{
    "version": 2,
    "name": "server",
    "builds": [
      { "src": "./server.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/data/getPrice",
        "methods": ["POST", "GET"],
        "headers": "Content-Type",
        "allow": ["https://csgobackpack.net"]
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }